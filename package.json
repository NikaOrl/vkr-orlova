{
  "name": "vkr-orlova",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test:ci": "ng test",
    "test": "ng test --watch",
    "lint:ts": "ng lint --format=stylish --fix",
    "lint:style": "stylelint \"src/app/**/*.{css,scss}\" --fix",
    "lint:prettier": "prettier --config .prettierrc --write src/**/*.{ts,scss,html}",
    "lint:commit": "node ./githooks/commit-msg.js",
    "lint": "npm run lint:ts && npm run lint:style && npm run lint:prettier",
    "docker:build": "docker build -t nikaorl/vkr-orlova .",
    "docker:db": "cd ./db && docker-compose up",
    "docker:db:down": "cd ./db && docker-compose down -v",
    "docker:run": "docker run --rm -it -p 4200:96 -e APP_SERVER=http://localhost:3000 nikaorl/vkr-orlova"
  },
  "dependencies": {
    "@angular/animations": "^11.2.6",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/material": "^11.2.5",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@ngneat/transloco": "^2.20.1",
    "file-saver": "^2.0.5",
    "rxjs": "~6.6.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.19",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@types/file-saver": "^2.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.35",
    "codelyzer": "^6.0.0",
    "husky": "^4.2.3",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "^0.0.32",
    "ng-mocks": "^11.9.1",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-selector-no-empty": "^1.0.8",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikaOrl/vkr-orlova"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/NikaOrl/vkr-orlova#readme",
  "husky": {
    "hooks": {
      "commit-msg": "npm run lint:commit",
      "pre-push": "npm run lint"
    }
  }
}
