{
  "name": "vkr-orlova",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-client": "ng serve --proxy-config proxy.conf.json",
    "start": "node ./bin/www",
    "build": "ng build",
    "test-once": "ng test",
    "test": "ng test --watch",
    "lint:ts": "ng lint --format=stylish --fix",
    "lint:style": "stylelint \"src/app/**/*.{css,scss}\" --fix",
    "lint:prettier": "prettier --config .prettierrc --write src/**/*.{ts,scss,html}",
    "lint:commit": "node ./githooks/commit-msg.js",
    "lint": "npm run lint:ts && npm run lint:style && npm run lint:prettier",
    "create-tables": "cd knex-helper/tables && node create_disciplines_table.js && node create_groups_table.js && node create_jobs_table.js && node create_marks_table.js && node create_students_disciplines_table.js && node create_students_table.js && node create_teachers_table.js",
    "insert-tables": "cd knex-helper/tables && node insert_disciplines.js && node insert_groups.js && node insert_jobs.js && node insert_marks.js && node insert_students_disciplines.js && node insert_students.js && node insert_teachers.js",
    "build-docker": "docker build --target build -t nikaorl/vkr-orlova .",
    "run-docker": " docker run --name nikaorl/vkr-orlova-container -d -p 3000:3000 -p 4200:4200 vkr-orlova"
  },
  "dependencies": {
    "@angular/animations": "^11.2.6",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/material": "^11.2.5",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "bcryptjs": "^2.3.0",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "http-errors": "^1.7.2",
    "jwt-simple": "^0.5.0",
    "knex": "^0.95.2",
    "moment": "^2.15.2",
    "morgan": "^1.9.1",
    "mysql2": "^2.2.5",
    "rxjs": "~6.6.6",
    "serve-favicon": "^2.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.35",
    "codelyzer": "^6.0.0",
    "husky": "^4.2.3",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-mocks": "^11.9.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-selector-no-empty": "^1.0.8",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikaOrl/vkr-orlova"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/NikaOrl/vkr-orlova#readme",
  "husky": {
    "hooks": {
      "commit-msg": "npm run lint:commit",
      "pre-commit": "npm run lint"
    }
  }
}
