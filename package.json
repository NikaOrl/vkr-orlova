  "scripts": {
    "ng": "ng",
    "start-client": "ng serve",
    "start": "node ./bin/www",
    "build": "ng build",
    "test-once": "ng test",
    "test": "ng test --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "create-tables": "cd knex-helper/tables && node create_disciplines_table.js && node create_groups_table.js && node create_jobs_table.js && node create_marks_table.js && node create_students_disciplines_table.js && node create_students_table.js && node create_teachers_table.js",
